-- Stored procedures para obtener listado de pedidos

CREATE DEFINER=`root`@`localhost` PROCEDURE `get_listado_pedidos`()
BEGIN
	SELECT * FROM PROYECTO_PEDIDOS.PEDIDO;
END

-- stored procedures para obtener listado de juegos

CREATE DEFINER=`root`@`localhost` PROCEDURE `get_listado_juegos`()
BEGIN
	SELECT * FROM PROYECTO_PEDIDOS.JUEGO;
END
